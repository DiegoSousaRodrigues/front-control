{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: Run Development Server",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "cwd": "${workspaceFolder}",
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "ready on http://localhost:3000",
        "uriFormat": "http://localhost:3000"
      }
    }
  ]
}
